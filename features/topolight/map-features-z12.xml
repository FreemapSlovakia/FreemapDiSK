<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>
<rules
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:svg="http://www.w3.org/2000/svg"
    svgBaseProfile="full"
    scale="1"
    symbolScale="1"
    minimumMapWidth="1"
    minimumMapHeight="1"
    withOSMLayers="yes"
    textAttenuation="0.3"
    withUntaggedSegments="no"
    showScale="no"
    showGrid="no"
    showBorder="no"
    showLicense="no"
    interactive="no"
	processWays="yes">

    <!--data_mkr1-->
    <data file="data.osm" />
    <!--data_mkr2-->

    <!--bounds_mkr1-->
    <!--bounds_mkr2-->

    <rule e="node|way" k="osmarender:render" v="~|yes">
        <rule e="way" k="ele" v="20|40|60|80" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="120|140|160|180" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="220|240|260|280" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="320|340|360|380" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="420|440|460|480" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="520|540|560|580" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="620|640|660|680" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="720|740|760|780" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="820|840|860|880" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="920|940|960|980" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1020|1040|1060|1080" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1120|1140|1160|1180" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1220|1240|1260|1280" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1320|1340|1360|1380" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1420|1440|1460|1480" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1520|1540|1560|1580" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1620|1640|1660|1680" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1720|1740|1760|1780" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1820|1840|1860|1880" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="1920|1940|1960|1980" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2020|2040|2060|2080" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2120|2140|2160|2180" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2220|2240|2260|2280" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2320|2340|2360|2380" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2420|2440|2460|2480" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2520|2540|2560|2580" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2620|2640|2660|2680" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2720|2740|2760|2780" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2820|2840|2860|2880" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>
        <rule e="way" k="ele" v="2920|2940|2960|2980" >
            <line class='topoline-generic topoline-minor' smart-linecap='no' />
        </rule>

        <rule e="way" k="ele" v="100|200|300|400|600|700|800|900|1100|1200|1300|1400|1600|1700|1800|1900|2100|2200|2300|2400|2600|2700|2800|2900" >
            <line class='topoline-generic topoline-mid' smart-linecap='no' />
        </rule>

        <rule e="way" k="ele" v="0|500|1000|1500|2000|2500|3000" >
            <line class='topoline-generic topoline-major' smart-linecap='no' />
        </rule>

        <rule e="way" k="ele" v="100|200|300|400|600|700|800|900|1100|1200|1300|1400|1600|1700|1800|1900|2100|2200|2300|2400|2600|2700|2800|2900" >
            <text k="ele" class="topoline-cas topoline-mid-cas" dy="2px" x-name-direction='normal' x-multi-labeling='57.143' text-postfix='m' />
            <text k="ele" class="topoline-cor topoline-mid-cor" dy="2px" x-name-direction='normal'  x-multi-labeling='57.143' text-postfix='m' />
        </rule>

        <rule e="way" k="ele" v="0|500|1000|1500|2000|2500|3000" >
            <text k="ele" class="topoline-cas topoline-major-cas" dy="2px" x-name-direction='normal' x-multi-labeling='57.143' text-postfix='m' />
            <text k="ele" class="topoline-cor topoline-major-cor" dy="2px" x-name-direction='normal'  x-multi-labeling='57.143' text-postfix='m' />
        </rule>

    </rule>
    <!-- k="osmarender:render" v="~|yes" -->

    <!-- SVG Definitions - markers, symbols etc go here -->
    <defs>
        <style id="styles" type="text/css" xmlns="http://www.w3.org/2000/svg">

            /* topoline - generic style */
            .topoline-generic { stroke-linecap: butt; stroke-linejoin: round; fill: none; }
            .topoline-minor { stroke-width: 0.11429px; stroke: #338800; }
            .topoline-mid { stroke-width: 0.2px; stroke: #444400; }
            .topoline-major { stroke-width: 0.2px; stroke: #770000; }


            /* topoline - texts */
            .topoline-cas { font-family: Tahoma; font-weight: normal; text-anchor: middle; method: stretch; spacing: auto; stroke: white; fill: white; letter-spacing: 4px; opacity: 0.3; stroke-width: 1.42857px; }
            .topoline-cor { fill: none; font-family: Tahoma; font-weight: normal; text-anchor: middle; method: stretch; spacing: auto; letter-spacing: 4px; }

            .topoline-minor-cas { font-size: 6.286px; stroke-width: 1.42857px; }
            .topoline-minor-cor { font-size: 6.286px; fill: #338800; }

            .topoline-mid-cas { font-size: 6.286px; stroke-width: 1.42857px; }
            .topoline-mid-cor { font-size: 6.286px; fill: #444400; }

            .topoline-major-cas { font-size: 6.286px; stroke-width: 1.42857px; }
            .topoline-major-cor { font-size: 6.286px; fill: #770000; }

            /* Map decoration */
            .map-grid-line { fill: none; stroke: #8080ff; stroke-width: 0.02857px; stroke-opacity: 0.5; }
            .map-border-cas { fill: none; stroke: #8080ff; stroke-width: 0.85714px; stroke-miterlimit: 4; stroke-dasharray: none; stroke-opacity: 1; stroke-linecap: round; }
            .map-border-cor { fill: none; fill-opacity: 1; fill-rule: nonzero; stroke: #ffffff; stroke-width: 0.57143px; stroke-miterlimit: 0; stroke-dashoffset: -0.14286px; stroke-opacity: 1; }
            .map-scale-cas { fill: none; stroke: #8080ff; stroke-width: 1.14286px; stroke-linecap: butt; }
            .map-scale-cor { fill: none; stroke: #ffffff; stroke-width: 0.85714px; stroke-linecap: butt; }
            .map-scale-bookend { fill: none; stroke: #8080ff; stroke-width: 0.28571px; stroke-linecap: butt; }
            .map-scale-caption { font-family: DejaVu Sans,sans-serif; font-size: 2.85714px; fill: #8080ff; }
            .map-background { fill: none; stroke: none; }
            .map-title { font-family: DejaVu Sans,sans-serif; font-size: 5.714px; text-anchor: middle; fill: black; }
            .map-title-background { fill: white; }
            .map-marginalia-background { fill: white; }
        </style>
    </defs>
</rules>
