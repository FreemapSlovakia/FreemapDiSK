	xmlns:svg="http://www.w3.org/2000/svg"
	svgBaseProfile="full"
	scale="1"
	symbolScale="1"
	minimumMapWidth="1"
	minimumMapHeight="1"
	withOSMLayers="yes"
	textAttenuation="0.0009"
	withUntaggedSegments="no"
	showScale="no"
	showGrid="no"
	showBorder="no"
	showLicense="no"
