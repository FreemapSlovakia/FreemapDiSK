<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>

<!-- This file should be used with Osmarender 4 -->
<!-- This file implements a sub-set of the items described at http://wiki.openstreetmap.org/index.php/Map_Features -->

<!-- A scale of 0.1 will make fat roads on a small map, a scale of 5 will draw very thin roads on a large scale map -->
<!-- minimumMapWidth/Height is in kilometres -->
<!-- Set javaScript="no" if you want an svg file that contains no javascript.  This is so that you can upload it to Wikipedia etc -->
<rules
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:svg="http://www.w3.org/2000/svg"
    scale="1"
    svgBaseProfile="full"
    symbolScale="1"
    minimumMapWidth="1"
    minimumMapHeight="1"
    withOSMLayers="yes"
   	textAttenuation="0.11"
    withUntaggedSegments="no"
    showScale="no"
    showGrid="no"
    showBorder="no"
    showLicense="no"
    interactive="no"
    processWays="yes">

    <!--data_mkr1-->
    <data file="data.osm" />
    <!--data_mkr2-->

	<!--bounds_mkr1-->
	<!--bounds_mkr2-->

	<!-- For debugging this rule draws a one pixel wide trace of *all* segments.  This enables segments that have no
            tags to be identified. Comment it out to hide the debug trace. -->

	<!-- ==================================== Layer -5 ======================================== -->
	<rule e="node|way" k="osmarender:render" v="~|yes" layer="-5">

		<!-- Non-physical boundaries -->
		<!-- Draw border features -->
		<rule e="way" k="boundary" v="administrative" layer="-5">
			<rule e="way" k="border_type" v="*" layer="-5">
				<rule e="way" k="border_type" v="state|nation" layer="-5">
					<line class='boundary boundary-adm-state'  bezier-hint='no' />
				</rule>

				<rule e="way" k="border_type" v="state|nation" layer="-5">
					<line class='boundary boundary-adm-state-2'  bezier-hint='no' />
				</rule>

				<rule e="way" k="border_type" v="region" layer="-5">
					<line class='boundary boundary-adm-region'  bezier-hint='no' />
				</rule>
				<rule e="way" k="border_type" v="region" layer="-5">
					<line class='boundary boundary-adm-region-2'  bezier-hint='no' />
				</rule>
			</rule>
		</rule>
	</rule>
	<!-- ==================================== END Layer -5 ======================================== -->

	<!-- Select all nodes, waysegments and ways (unless they have an osmarender:render=no tag) -->
	<!-- If you really want to render segments then you will need to add segment to the element list, but please don't - tag the ways instead. -->
	<rule e="node|way" k="osmarender:render" v="~|yes">

		<!-- Natural features -->
		<layer name="natural" >
			<rule e="way" k="natural" v="coastline">
				<line class='natural-coastline'/>
			</rule>
			<rule e="way" k="natural" v="land">
				<area class='natural-land' />
			</rule>
			<rule e="way" k="landuse|natural" v="forest|wood">
				<area class='landuse-wood'/>
			</rule>

			<rule e="way" k="bridge" v="~">
				<rule e="way" k="tunnel" v="~">
					<!-- main part of abuters in uniform color -->
					<rule e="way" k="highway" v="residential">
						<line class='abutters-all'  smart-linecap='no'/>
					</rule>
					<rule e="way" k="abutters" v="residential|retail|industrial|commercial|mixed">
						<line class='abutters-all' smart-linecap='no' />
					</rule>
				</rule>
			</rule>


			<!-- Landuse -->
			<rule e="way" k="landuse|natural" v="*">
				<!-- at zoom 12 render all landuse in the same colour so it doesn't look too psychadelic -->
				<rule e="way" k="landuse" v="residential|retail|industrial|commercial|cemetery">
					<area class='landuse-all' bezier-hint='no' />
				</rule>
			</rule>



			<rule e="way" k="landuse" v="field|meadow">
				<area class='landuse-field'/>
			</rule>
			<rule e="way" k="leisure" v="park|playing_fields|garden|pitch|golf_course|common|green">
				<area class='leisure-park' bezier-hint='no' />
			</rule>
			<rule e="way" k="leisure" v="stadium|sports_centre">
				<area class='leisure-stadium'/>
			</rule>
			<rule e="way" k="leisure" v="track">
				<area class='leisure-trk'/>
			</rule>
		</layer>


		<!-- Airports and runways -->
		<rule e="way" k="aeroway" v="apron">
			<area class='aeroway-apron' bezier-hint='no' />
		</rule>
		<rule e="way" k="landuse" v="runway">
			<rule e="way" k="use_status" v="~">
				<area class='landuse-runway'  bezier-hint='no' />
			</rule>
			<rule e="way" k="use_status" v="disused">
				<area class='landuse-runway-disused'  bezier-hint='no' />
			</rule>
			<rule e="way" k="use_status" v="dismantled">
				<area class='landuse-runway-dismantled'  bezier-hint='no' />
			</rule>
		</rule>

		<!-- Waterways, lakes and ponds -->
		<rule e="way" k="waterway" v="riverbank">
			<area class='wwy-riverbank'/>
		</rule>
		<rule e="way" k="waterway" v="river">
			<line class='wwy-river-cas'/>
		</rule>

		<rule e="way" k="waterway" v="river">
			<line class='wwy-river-cor'/>
		</rule>
		<rule e="way" k="natural" v="water|pond|lake">
			<area class='natural-water' />
		</rule>
		<rule e="way" k="landuse" v="reservoir">
			<area class='natural-water' />
		</rule>
		<rule e="way" k="landuse" v="basin">
			<area class='natural-water' />
		</rule>
	</rule>
	<!-- k="osmarender:render" v="~|yes" -->

	<!-- ==================================== Layer 5 ======================================== -->
	<rule e="node|way" k="osmarender:render" v="~|yes" layer="5">

		<!-- Non-physical boundaries -->
		<!-- Draw border features -->
		<rule e="way" k="boundary" v="administrative" layer="5">
			<rule e="way" k="border_type" v="*" layer="5">
				<rule e="way" k="border_type" v="state|nation" layer="5">
					<line class='boundary boundary-adm-state-cor'  bezier-hint='no' />
				</rule>
				<rule e="way" k="border_type" v="region" layer="5">
					<line class='boundary boundary-adm-region-cor' bezier-hint='no' />
				</rule>
			</rule>
			<else>
				<line class='boundary boundary-adm-region-cor' bezier-hint='no' />
			</else>
		</rule>
	</rule>
	<!-- ==================================== END Layer 5 ======================================== -->


