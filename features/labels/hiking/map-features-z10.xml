/* Map decoration */
<!-- map background must be the same for all zooms or else empty tile detection will fail -->
.map-background {
	fill: none;
	stroke: none;
}
.map-border-cas {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.21429px;
	stroke-miterlimit: 4;
	stroke-dasharray: none;
	stroke-opacity: 1;
	stroke-linecap: round;
}
.map-border-cor {
	fill: none;
	fill-opacity: 1;
	fill-rule: nonzero;
	stroke: #ffffff;
	stroke-width: 0.14286px;
	stroke-miterlimit: 0;
	stroke-dashoffset: -0.03571px;
	stroke-opacity: 1;
}
.map-grid-line {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.00714px;
	stroke-opacity: 0.5;
}
.map-marginalia-background {
	fill: white;
}
.map-scale-bookend {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.07143px;
	stroke-linecap: butt;
}
.map-scale-caption {
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 0.71429px;
	fill: #8080ff;
}
.map-scale-cas {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.28571px;
	stroke-linecap: butt;
}
.map-scale-cor {
	fill: none;
