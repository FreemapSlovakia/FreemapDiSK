<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>

<rules
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:svg="http://www.w3.org/2000/svg"
	xmlns:sodipodi="http://sodipodi.sourceforge.net/DTD/sodipodi-0.dtd"
	xmlns:inkscape="http://www.inkscape.org/namespaces/inkscape"
	scale="1"
	svgBaseProfile="full"
	symbolScale="1"
	minimumMapWidth="1"
	minimumMapHeight="1"
	withOSMLayers="yes"
	showScale="no"
	showGrid="no"
	showBorder="no"
	showLicense="no"
	interactive="no"
	processWays="no">

	<!--data_mkr1-->
	<data file="data.osm" />
	<!--data_mkr2-->

	<!--bounds_mkr1-->
	<!--bounds_mkr2-->

	<rule e="node|way" k="osmarender:render" v="~|yes">
		<!-- Place names -->
		<rule e="node" k="fmlabz8" v="1">
			<!--<rule e="node" k="type" v="place_village">
				<text k='name' class='village-caption-cas' x-line-spacing='11.607' dy='-4.46429px' x-text-block-anchor='top' />
				<text k='name' class='village-caption-cor' x-line-spacing='11.607' dy='-4.46429px' x-text-block-anchor='top' />
			</rule>-->
			<rule e="node" k="type" v="place_town">
				<text k='name' class='town-caption-cas' x-line-spacing='12.5' dy='-4.46429px' x-text-block-anchor='top' />
				<text k='name' class='town-caption-cor' x-line-spacing='12.5' dy='-4.46429px' x-text-block-anchor='top' />
			</rule>
			<rule e="node" k="type" v="place_county">
				<text k='name' class='county-town-caption-cas' x-line-spacing='13.393' dy='-4.46429px' x-text-block-anchor='top' />
				<text k='name' class='county-town-caption-cor' x-line-spacing='13.393' dy='-4.46429px' x-text-block-anchor='top' />
			</rule>
			<rule e="node" k="type" v="place_region">
				<text k='name' class='region-city-caption-cas' x-line-spacing='14.286' dy='-4.46429px' x-text-block-anchor='top' />
				<text k='name' class='region-city-caption-cor' x-line-spacing='14.286' dy='-4.46429px' x-text-block-anchor='top' />
			</rule>
			<rule e="node" k="type" v="place_city">
				<text k='name' class='city-caption-cas' x-line-spacing='15.714' dy='-4.46429px' x-text-block-anchor='top' />
				<text k='name' class='city-caption-cor' x-line-spacing='15.714' dy='-4.46429px' x-text-block-anchor='top' />
			</rule>
		</rule>
	</rule>
	<!-- k="osmarender:render" v="~|yes" -->

	<!-- SVG Definitions - markers, symbols etc go here -->
	<defs>

		<svg:filter
			inkscape:collect="always"
			id="gauss7"
			x="-0.030957849"
			width="1.0619157"
			y="-0.15485379"
			height="1.3097076">
			<svg:feGaussianBlur
				inkscape:collect="always"
				stdDeviation="1.3"
				id="feGaussianBlur7" />
		</svg:filter>

		<svg:filter
			inkscape:collect="always"
			id="gauss5"
			x="-0.030957849"
			width="1.0619157"
			y="-0.15485379"
			height="1.3097076">
			<svg:feGaussianBlur
				inkscape:collect="always"
				stdDeviation="0.7"
				id="feGaussianBlur5" />
		</svg:filter>

		<style id="styles" type="text/css" xmlns="http://www.w3.org/2000/svg">

/* features */
.city-caption-cas {
	fill: white;
	stroke: white;
	stroke-width: 3.571px;
	font-family: Myriad Pro Black;
	-inkscape-font-specification: Myriad Pro Black;
	font-size: 17.857px;
	font-weight: normal;
	text-anchor: middle;
	filter: url(#gauss7);
}
.city-caption-cor {
	fill: black;
	stroke: none;
	font-family: Myriad Pro Black;
	-inkscape-font-specification: Myriad Pro Black;
	font-size: 17.857px;
	font-weight: normal;
	text-anchor: middle;
}
.county-town-caption-cas {
	fill: white;
	stroke: white;
	stroke-width: 3.571px;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 15.179px;
	font-weight: normal;
	text-anchor: middle;
	filter: url(#gauss7);
}
.county-town-caption-cor {
	fill: black;
	stroke: none;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 15.179px;
	font-weight: normal;
	text-anchor: middle;
}
/* Map decoration */
<!-- map background must be the same for all zooms or else empty tile detection will fail -->
.map-background {
	fill: none;
	stroke: none;
}
.map-border-cas {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.05357px;
	stroke-miterlimit: 4;
	stroke-dasharray: none;
	stroke-opacity: 1;
	stroke-linecap: round;
}
.map-border-cor {
	fill: none;
	fill-opacity: 1;
	fill-rule: nonzero;
	stroke: #ffffff;
	stroke-width: 0.03571px;
	stroke-miterlimit: 0;
	stroke-dashoffset: -0.00893px;
	stroke-opacity: 1;
}
.map-grid-line {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.00179px;
	stroke-opacity: 0.5;
}
.map-marginalia-background {
	fill: white;
}
.map-scale-bookend {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.01786px;
	stroke-linecap: butt;
}
.map-scale-caption {
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 0.17857px;
	fill: #8080ff;
}
.map-scale-cas {
	fill: none;
	stroke: #8080ff;
	stroke-width: 0.07143px;
	stroke-linecap: butt;
}
.map-scale-cor {
	fill: none;
	stroke: #ffffff;
	stroke-width: 0.05357px;
	stroke-linecap: butt;
}
.map-title {
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 0.35714px;
	text-anchor: middle;
	fill: black;
}
.map-title-background {
	fill: white;
}
.region-city-caption-cas {
	fill: white;
	stroke: white;
	stroke-width: 3.571px;
	font-family: Myriad Pro Black;
	-inkscape-font-specification: Myriad Pro Black;
	font-size: 16.071px;
	font-weight: normal;
	text-anchor: middle;
	filter: url(#gauss7);
}
.region-city-caption-cor {
	fill: black;
	stroke: none;
	font-family: Myriad Pro Black;
	-inkscape-font-specification: Myriad Pro Black;
	font-size: 16.071px;
	font-weight: normal;
	text-anchor: middle;
}
.town-caption-cas {
	fill: white;
	stroke: white;
	stroke-width: 3.036px;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 14.286px;
	font-weight: normal;
	text-anchor: middle;
	filter: url(#gauss7);
}
.town-caption-cor {
	fill: black;
	stroke: none;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 14.286px;
	font-weight: normal;
	text-anchor: middle;
}
.village-caption-cas {
	fill: white;
	stroke: white;
	stroke-width: 3.036px;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 10.714px;
	font-weight: normal;
	text-anchor: middle;
	filter: url(#gauss5);
}
.village-caption-cor {
	fill: black;
	stroke: none;
	font-family: Myriad Pro;
	-inkscape-font-specification: Myriad Pro;
	font-size: 10.714px;
	font-weight: normal;
	text-anchor: middle;
}
		</style>
	</defs>
</rules>
